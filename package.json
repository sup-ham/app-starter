{
  "name": "@yusham/blangko",
  "description": "Start business from a blank project",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify --pages src_front/pages",
    "dev:vite": "vite",
    "start": "bun src_api/app.ts",
    "build": "run-s build:*",
    "build:routify": "routify -b --pages src_front/pages",
    "build:vite": "vite build",
    "postinstall": "node -e 'require(\"./svelte.config.js\").copyConfig()'"
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.8",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "bun-types": "^0.1.5",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.49.0",
    "svelte-paginate": "^0.0.1",
    "svelte-preprocess": "^4.10.7",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "cookie": "^0.5.0",
    "jsonwebtoken": "^8.5.1"
  }
}
