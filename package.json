{
  "name": "@yusham/blangko",
  "description": "Start business from a blank project",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify --pages src_front/pages",
    "dev:vite": "vite",
    "start": "polkadot server.js",
    "build": "run-s build:*",
    "build:routify": "routify -b --pages src_front/pages",
    "build:vite": "vite build",
    "postinstall": "node -e 'require(\"./svelte.config.js\").copyConfig()'"
},
  "devDependencies": {
    "@roxi/routify": "^2",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.48",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.48.0",
    "svelte-paginate": "^0.0.1",
    "svelte-preprocess": "^4.10.7",
    "vite": "^2.9.10"
  },
  "dependencies": {
    "polkadot": "^1.0.0",
    "serve-static": "^1.15.0"
  }
}
