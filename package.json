{
  "name": "@yusham/blangko",
  "description": "Start business from a blank project",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify --pages src_front/pages",
    "dev:vite": "vite",
    "start": "polkadot src_api/app.js",
    "build": "run-s build:*",
    "build:routify": "routify -b --pages src_front/pages",
    "build:vite": "vite build",
    "postinstall": "node -e 'require(\"./svelte.config.js\").copyConfig()'"
},
  "devDependencies": {
    "@roxi/routify": "^2",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.39",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.46.4",
    "svelte-paginate": "^0.0.1",
    "svelte-preprocess": "^4.10.4",
    "vite": "^2.8.6"
  },
  "dependencies": {
    "polkadot": "^1.0.0",
    "raw-body": "^2.5.1",
    "send": "^0.18"
  }
}
